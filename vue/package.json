{
  "name": "vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@aurium/vue-plotly": "^2.0.0-rc5",
    "@mdi/font": "^7.4.47",
    "core-js": "^3.39.0",
    "jspdf": "^3.0.1",
    "vue": "^3.5.13",
    "vue-html2canvas": "^0.0.4",
    "vue-i18n": "^11.1.2",
    "vue-inline-svg": "^3.1.4",
    "vue-router": "^4.5.0",
    "vuetify": "^3.7.6",
    "vuex": "^4.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^9.32.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off",
      "no-unused-vars": "off",
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        2,
        "double",
        {
          "avoidEscape": true
        }
      ]
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
